<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: #8baddb;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <ImageView fitHeight="450.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../OfficialDownload.jpg" />
         </image>
      </ImageView>
      <Label layoutX="36.0" layoutY="125.0" prefHeight="18.0" prefWidth="113.0" text="Username">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="35.0" layoutY="194.0" prefHeight="35.0" prefWidth="114.0" text="Password">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="Notify" layoutX="19.0" layoutY="378.0" prefHeight="46.0" prefWidth="467.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="UserText" layoutX="168.0" layoutY="130.0" prefHeight="26.0" prefWidth="276.0" />
      <TextField fx:id="PassText" layoutX="168.0" layoutY="198.0" prefHeight="26.0" prefWidth="276.0" />
      <Label layoutX="-1.0" layoutY="-2.0" prefHeight="92.0" prefWidth="515.0" text="Customer Login" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="65.0" />
         </font>
      </Label>
      <Button fx:id="GoToPage" layoutX="116.0" layoutY="323.0" mnemonicParsing="false" onAction="#CheckLogin" prefHeight="35.0" prefWidth="52.0" text="Login" />
      <Button fx:id="goBack" layoutX="313.0" layoutY="328.0" mnemonicParsing="false" onAction="#Welcome" prefHeight="35.0" prefWidth="68.0" text="Go Back" />
   </children>
</AnchorPane>
